{
  "name": "artreader",
  "version": "0.0.2",
  "description": "Reader for PDF, djvi with notes and bookmarks",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf dist && rimraf node_modules",
    "build:render": "webpack -c ./build/webpack.renderer.dev.js --progress --mode development",
    "watch:render": "npm run build:render && webpack serve --config ./build/webpack.renderer.dev.js --mode development --hot",
    "build:electron": "webpack -c ./build/webpack.electron.dev.js --progress --mode development",
    "launch": "electron ./dist/main.js",
    "build:launch": "npm run build:render && npm run build:electron && electron ./dist/main.js",
    "eslint": "eslint --ext .ts,.vue src",
    "test": "echo Error Fish:D",
    "build:release": "npm run build:render && npm run build:electron && electron-builder -w -c ./build/electron-builder.release.js"
  },
  "author": "Team ArtReader",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.24.0",
    "@quasar/app-webpack": "^4.2.0",
    "@quasar/extras": "^1.16.17",
    "@types/node": "^22.14.0",
    "@types/pdfjs-dist": "^2.10.377",
    "@vue/cli-service": "^3.3.1",
    "@vue/runtime-core": "^3.5.13",
    "autoprefix": "^1.0.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.5.0",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "electron-log": "^5.3.3",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "node-loader": "^2.1.0",
    "pinia": "^3.0.2",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vue": "^3.5.13",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "vuex": "^4.1.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "pdfjs-dist": "^5.1.91",
    "quasar": "^2.18.1"
  }
}